{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.chicken.general": {
			"states": {
				"default": {
					"animations": [
						"idle"
					],
					"transitions": [
						{
							"walking": "query.modified_move_speed > 0.01"
						}
					],
					"blend_transition": 0.3
				},
				"walking": {
					"animations": [
						{
							"walk": "query.modified_move_speed"
						}
					],
					"transitions": [
						{
							"default": "query.modified_move_speed <= 0.01"
						}
					],
					"blend_transition": 0.3
				}
			}
		}
	}
}